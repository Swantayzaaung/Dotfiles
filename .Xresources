!! Colorscheme

! Special
*.foreground: #bde7ee
*.background: #031c28
!*.background: #022136
*.cursorColor: #afbfbf

! a blue colorsscheme
*.color0: #022136
*.color1: #104162
*.color2: #25648f
*.color3: #367dab
*.color4: #539ac7
*.color5: #78c9df
*.color6: #bde7ee 
*.color7: #e7fffb

*.color8: #022136
*.color9: #104162
*.color10: #25648f
*.color11: #367dab
*.color12: #539ac7
*.color13: #78c9df
*.color14: #bde7ee
*.color15: #e7fffb

! Fonts
*.faceName: DejaVu Sans Mono 
*.faceSize: 11

! Urxvt stuff
URxvt.geometry: 100x40
URxvt.font: xft:DejaVu Sans Mono:size=10:xft:Tinos Nerd Font Complete:size=10:style=Book
!URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: true

! Locale
*.vt100.locale: false
*.vt100.utf8: true

! Scale
!Xft.dpi: 120

! Keybinds
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
